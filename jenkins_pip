pipeline{
    agent any

    environment{
        PATH ="/opt/maven/bin:$PATH"
    }
    stages{
        stage("git check"){
            steps{
                git credentialsId: 'git_id', url: 'https://github.com/sasigowda/hello-world'
            }           
        }
        stage("maven build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
